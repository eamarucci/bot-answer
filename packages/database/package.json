{
  "name": "@botanswer/database",
  "version": "1.0.0",
  "description": "Database package with Prisma for BotAnswer",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "generate": "dotenv -e ../../.env -- prisma generate",
    "push": "dotenv -e ../../.env -- prisma db push",
    "migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "studio": "dotenv -e ../../.env -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "dotenv-cli": "^8.0.0",
    "prisma": "^6.2.1",
    "typescript": "^5.7.2"
  }
}
